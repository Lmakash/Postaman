				{
					"listen": "test",
					"script": {
						"exec": [
							"pm.test(\"Status code is 200\", function () {\r",
							"    pm.response.to.have.status(200);\r",
							"});\r",
							"// var jdata=JSON.parse(responseBody)\r",
							"// tests[\"verify first name:\"]=(jdata.data[1].first_name==\"Linday\")\r",
							"    \r",
							"\r",
							" pm.test(\"verify the employee data\", function () {\r",
							"  //  var jsondata=pm.response.json;\r",
							"  var jdata=JSON.parse(responseBody);\r",
							"   pm.expect(jdata.data[1].id).to.eql(8);\r",
							"    pm.expect(jdata.data[1].email).to.eql(\"lindsay.ferguson@reqres.in\");\r",
							"      pm.expect(jdata.data[1].first_name).to.eql(\"Lindsay\");\r",
							"     pm.expect(jdata.data[1].last_name).to.eql(\"Ferguson\");\r",
							"     pm.expect(jdata.data[1].avatar).to.eql(\"https://reqres.in/img/faces/8-image.jpg\");\r",
							" });\r",
							"\r",
							"\r",
							"\r",
							" pm.test(\"verify the data\", function () {\r",
							" var jdata=JSON.parse(responseBody);\r",
							" var firstName=jdata.data[1].first_name;\r",
							" var lastName=jdata.data[1].last_name;\r",
							" var fullname=firstName.concat(\".\");\r",
							" var fullname=fullname.concat(lastName);\r",
							" var email=fullname.concat(\"@reqres.in\")\r",
							"  });\r",
							"\r",
							"\r",
							"  pm.test(\"content type validation\", function () {\r",
							"pm.expect(pm.response.headers.get(\"content Type\")).to.eql(\"application/js0; charset=utf-8\");\r",
							"});"
						],
						"type": "text/javascript"
					}
				}
